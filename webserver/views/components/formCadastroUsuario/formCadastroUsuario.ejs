<form action="/usuarios" method="post">
    <div class="form-group">
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="E-mail" name="email">
    </div>
    <div class="form-row">
        <div class="col">
            <input type="text" class="form-control" id="exampleInputName1" aria-describedby="nameHelp" placeholder="Nome" name="nome">
        </div>
        <div class="col">
            <div class="form-group">
                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Senha" name="senha">
            </div>
        </div>
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Aceito os <a target="_blank" rel="noopener noreferrer" href="http://www.google.com">Termos de Uso</a>.</label>
    </div>
    <button type="submit" class="btn btn-primary">Cadastrar</button>
</form>